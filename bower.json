{
	"name": "node-vpdb",
	"version": "0.0.5",
	"homepage": "https://github.com/freezy/node-vpdb",
	"authors": [ "freezy <freezy@xbmc.org>" ],
	"description": "A database for Visual Pinball resources",
	"main": "app.js",
	"moduleType": [
		"node"
	],
	"license": "GPLv2",
	"private": true,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"dependencies": {
		"jquery": "~2.1.1",
		"angular": "1.3",
		"angular-route": "1.3",
		"angular-animate": "1.3",
		"angular-sanitize": "1.3",
		"angular-resource": "1.3",
		"ng-file-upload": "~1.6.5",
		"angular-dragdrop": "~1.0.7",
		"angulartics": "~0.16.1",
		"underscore": "~1.6.0",
		"showdown": "~0.3.1",
		"magnific-popup": "~0.9.9",
		"nanoscroller": "~0.8.1",
		"waitForImages": "alexanderdickson/waitForImages#~1.4.2",
		"jquery-throttle-debounce": "*",
		"angular-scrollable": "~0.2.5",
		"ngstorage": "~0.3.0",
		"angular-bootstrap": "~0.11.0",
		"fontawesome": "~4.1.0"
	},
	"overrides": {
		"showdown": {
			"main": [
				"src/showdown.js",
				"src/extensions/github.js"
			]
		},
		"jquery-ui": {
			"main": [
				"ui/core.js",
				"ui/widget.js",
				"ui/mouse.js",
				"ui/draggable.js",
				"ui/droppable.js"
			]
		},
		"angulartics": {
			"main": [
				"src/angulartics.js",
				"src/angulartics-ga.js",
				"src/angulartics-ga-cordova.js"
			]
		},
		"perfect-scrollbar": {
			"main": [
				"src/jquery.mousewheel.js",
				"src/perfect-scrollbar.js",
				"src/perfect-scrollbar.css"
			]
		},
		"jquery-throttle-debounce": {
			"main": "jquery.ba-throttle-debounce.js"
		},
		"waitForImages": {
			"main": "src/jquery.waitforimages.js"
		}
	}
}
