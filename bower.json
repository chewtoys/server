		"angular-sanitize": "1.4.1",
		"angular-elastic": "^2.4.2",
		"angular-bootstrap": "0.13.0",
		"angular-moment": "0.10.1",
		"angular-dragdrop": "1.0.11",
		"angular-scrollable": "^0.2.5",
		"angular-scroll": "0.7.1",
		"angular-nanoscroller": "^0.2.1",
		"angular-ui-router": "0.2.15",
		"angulartics": "0.18.0",
		"angular-gravatar": "0.3.1",
		"angular-growl-notifications": "~2.1.2",
		"ng-file-upload": "5.0.7",
		"ng-clip": "0.2.6",
		"ngstorage": "^0.3.0",
