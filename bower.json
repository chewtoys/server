		"waitForImages": "alexanderdickson/waitForImages#~1.4.2",
		"jquery-throttle-debounce": "*",
		"highlightjs": "^8.4.0",
		"bootstrap": "3.3.5",
		"FileAPI": "^2.0.9",
		"lodash": "3.9.3",
		"traverse": "^0.6.7",
		"videojs": "4.12.9",
		"videojs-sublime-skin": "1.0.3",
		"bower-parseuri": "*",
		"object-path": "~0.8.1",
		"zeroclipboard": "2.2.0"
	},
	"overrides": {
		"bootstrap": {
			"main": [
				"dist/css/bootstrap.css"
			]
		},
		"showdown": {
			"main": [
				"src/showdown.js",
				"src/extensions/github.js"
			]
		},
		"jquery-ui": {
			"main": [
				"ui/core.js",
				"ui/widget.js",
				"ui/mouse.js",
				"ui/draggable.js",
				"ui/droppable.js"
			]
		},
		"angulartics": {
			"main": [
				"src/angulartics.js",
				"src/angulartics-ga.js",
				"src/angulartics-ga-cordova.js"
			]
		},
		"perfect-scrollbar": {
			"main": [
				"src/jquery.mousewheel.js",
				"src/perfect-scrollbar.js",
				"src/perfect-scrollbar.css"
			]
		},
		"angular-scroll": {
			"main": "angular-scroll.js"
		},
		"ng-file-upload": {
			"main": [
				"ng-file-upload.js",
				"ng-file-upload-shim.js"
			]
		},
		"jquery-throttle-debounce": {
			"main": "jquery.ba-throttle-debounce.js"
		},
		"waitForImages": {
			"main": "src/jquery.waitforimages.js"
