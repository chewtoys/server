@font-face {
	font-family: 'langdon';
	src: url('/fonts/langdon-webfont.eot');
	src: url('/fonts/langdon-webfont.eot?#iefix') format('embedded-opentype'),
	url('/fonts/langdon-webfont.woff') format('woff'),
	url('/fonts/langdon-webfont.ttf') format('truetype'),
	url('/fonts/langdon-webfont.svg#langdonregular') format('svg');
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'icons';
	src:url('/fonts/icons.eot?-ee3kf5');
	src:url('/fonts/icons.svg?-ee3kf5#icons') format('svg'),
	url('/fonts/icons.eot?#iefix-ee3kf5') format('embedded-opentype'),
	url('/fonts/icons.woff?-ee3kf5') format('woff'),
	url('/fonts/icons.ttf?-ee3kf5') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.glyphicon {
	font-family: FontAwesome;
}

[class^="icon-"], [class*=" icon-"] {
